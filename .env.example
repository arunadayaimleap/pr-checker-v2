# OpenRouter API key - required for accessing the models
# Starts with 'sk-or-'
OPENROUTER_API_KEY=your_openrouter_api_key

# GitHub personal access token with repo permissions
# Needed for posting comments to PRs
GITHUB_TOKEN=your_github_token

# Comment settings
COMMENT_ON_PR=true
COMBINED_DIAGRAM_COMMENT=false

# Path to draw.io (if using local installation)
DRAWIO_PATH=draw.io